submodule ietf-NOSServiceSettings {
	yang-version 1.1;
	belongs-to ietf-SystemIM {
		prefix "NOSSrvSetYang";
	}
	
	include ietf-NOSServiceCapabilities;

	organization "University of Cauca";
	contact 
		"Ana Montoya 
		Phone: +57 301 630 97331
		Email: anitamont7@gmail.com

		Daniela M. Casas
		Phone: +57 317 777 7225
		Email: danielacasas_@ieee.org

		Oscar Mauricio Caicedo
		Phone: +57 +57 310 491 8132
		Email: omcaicedo@unicauca.edu.co

		University of Cauca 
		Calle 5 #4-70 Popay√°n, Cauca
		Colombia";

	description "Submodule that represents the configuration parameters of
	the NOS service";

	revision 2017-04-12 {
		description "Initial revision.";
		reference "RFC 7950 - The YANG 1.1 Data Modeling Language";
	}

	leaf-list NOSConfParameters {
		type string;
		description "Represents the configuration parameters of the NOS";
	}
	leaf-list SettingsDefineCapabilities {
		type leafref {
			path "/nosServiceCapabilities";
		}
		description "Referred to the nosServiceCapabilities node for representing the 
		relationship with the NOSServiceCapabilities submodule";
	}
	leaf Logging{
		type string;
		description "Allows setting the method for storing data generated by the NOSService";
	}
}
